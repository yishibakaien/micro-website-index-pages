.footer
  background-color rgb(232, 232, 232)

  p
    color rgba(180, 180, 180, .8)

.main
  background-color #4c93fd
  
.sticky-blank
  background-color #fff

.logo-container
  height 156px
  width 156px
  background-color #f2f2f2
  border-radius 50%
  margin 0 auto
  position relative
  bottom -78px

.logo-wrapper
  background-color #fff
  -webkit-box-sizing border-box
  box-sizing border-box
  border 4px solid #4c93fd
  border-radius 50%
  position absolute
  left 4px
  right 4px
  top 4px
  bottom 4px
  display flex
  align-items center
  justify-content center
  overflow hidden

.logo
  height 90px
  width 90px
  background-image url('../../images/template_6_logo.png')
  background-size 100% 100%
  background-repeat no-repeat
  background-position center

.list-container
  width 100%
  background-color #fff
  overflow hidden
  padding-top 78px

  .list
    width 100px
    height 44px

    .iconfont
      color #9a99ff
      display inline-block
      height 40px
      width 40px
      font-size 24px
      line-height 38px
      text-align center
      -webkit-box-sizing border-box
      box-sizing border-box
      border 2px solid #9a99ff
      border-radius 50%

    .text
      color #666
      font-size 12px
      line-height 40px

  .list:nth-child(1)
    padding-top 20px

  .list:nth-child(6)
    padding-bottom  30px

  .list:nth-child(1), .list:nth-child(3), .list:nth-child(5)
    margin-left 50%
    padding-left 13px
    position relative
    left -1px
    border-left 2px solid #f2f2f2
    clear both

    .iconfont
      float left

    .text
      float right

  .list:nth-child(2), .list:nth-child(4), .list:nth-child(6)
    float right
    margin-right 50%
    padding-right 13px
    position relative
    right -1px
    border-right 2px solid #f2f2f2

    .iconfont
      float right

    .text
      float left

  .list:nth-child(1):before, .list:nth-child(3):before, .list:nth-child(5):before
    content '.'
    color #f2f2f2
    font-size 64px
    position absolute
    left -9px
    bottom 12px

  .list:nth-child(2):after, .list:nth-child(4):after, .list:nth-child(6):after
    content '.'
    color #f2f2f2
    font-size 64px
    position absolute
    right -9px
    top -30px








